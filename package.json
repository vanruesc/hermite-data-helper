{
	"name": "hermite-data-helper",
	"version": "1.0.2",
	"description": "A Hermite data visualization tool for rabbit-hole and three.",
	"homepage": "https://github.com/vanruesc/hermite-data-helper",
	"main": "build/hermite-data-helper.js",
	"module": "build/hermite-data-helper.esm.js",
	"license": "Zlib",
	"keywords": [
		"hermite",
		"data",
		"helper",
		"three",
		"rabbit-hole",
		"3d",
		"geometry",
		"mesh"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/hermite-data-helper.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/hermite-data-helper/issues"
	},
	"files": [
		"build"
	],
	"scripts": {
		"ava": "ava",
		"lint": "npm run lint:lib && npm run lint:test",
		"lint:config": "eslint *.js",
		"lint:lib": "eslint src",
		"lint:test": "eslint test",
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production rollup -c",
		"watch": "rollup -c --watch",
		"doc": "esdoc",
		"pretest": "npm run lint && npm run build:production",
		"test": "ava",
		"prepack": "npm test && npm run doc"
	},
	"ava": {
		"failFast": true,
		"files": [
			"test/**/*.js"
		],
		"require": [
			"esm"
		]
	},
	"eslintConfig": {
		"extends": "aether"
	},
	"peerDependencies": {
		"three": ">= 0.110.0 < 0.114.0"
	},
	"devDependencies": {
		"@babel/core": "7.x.x",
		"@babel/preset-env": "7.x.x",
		"@rollup/plugin-node-resolve": "7.x.x",
		"ava": "3.x.x",
		"cross-env": "7.x.x",
		"esdoc": "1.x.x",
		"esdoc-importpath-plugin": "1.x.x",
		"esdoc-standard-plugin": "1.x.x",
		"eslint": "6.x.x",
		"eslint-config-aether": "1.x.x",
		"esm": "3.x.x",
		"rollup": "1.x.x",
		"rollup-plugin-babel": "4.x.x",
		"rollup-plugin-terser": "5.x.x",
		"three": "0.113.x"
	}
}
